{
  "name": "plaisio/console-kernel",
  "description": "PhpPlaisio: Console Kernel",
  "keywords": [
    "PhpPlaisio",
    "Console",
    "Kernel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-dom": "*",
    "plaisio/console": "3.0.0",
    "plaisio/kernel": "^3.2.0",
    "setbased/error-handler": "^1.3.0",
    "setbased/exception": "^2.3.0",
    "setbased/helper-cast": "^3.0.0",
    "setbased/typed-config": "^2.0.0",
    "symfony/console": "^6.2.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.2"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Console\\Kernel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Console\\Kernel\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
